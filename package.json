{
  "name": "fr24api-sdk-js",
  "version": "1.0.0",
  "description": "Node.js SDK for Flightradar24 API",
  "author": "",
  "license": "MIT",
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
