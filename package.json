{
  "name": "fr24api-sdk-js",
  "version": "1.0.0",
  "description": "Node.js SDK for Flightradar24 API",
  "author": "",
  "license": "MIT",

  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },

  "files": ["dist"],
  "sideEffects": false,

  "scripts": {
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "esbuild src/index.js --platform=node --bundle --format=cjs --outfile=dist/index.cjs",
    "build:esm": "esbuild src/index.js --platform=node --bundle --format=esm --outfile=dist/index.mjs --banner:js=\"import { createRequire } from 'module'; const require = createRequire(import.meta.url);\"",
    "prepublishOnly": "npm run build"
  },

  "dependencies": {
    "axios": "^1.4.0",
    "dotenv": "^16.5.0",
    "qs": "^6.11.0"
  },
  "devDependencies": {
    "esbuild": "^0.23.0"
  }
}